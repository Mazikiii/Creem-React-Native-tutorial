# Creem API key, this is found in your Creem dashboard under Developers
# Use the test key (creem_test_...) during development
CREEM_API_KEY=

# Creem webhook secret this is found in Developers > Webhook in your dashboard
# Generated when you register your webhook endpoint
CREEM_WEBHOOK_SECRET=


// Add this later, after you get your url from railway, at the beginning you can put a random thing
// but when you get the url edit it
BACKEND_URL=https://creem-react-native-tutorial-production.up.railway.app


# The product ID for "Quill Pro", copy from your Creem dashboard Products tab
CREEM_PRODUCT_ID=

# Server port
PORT=3000

# Set to "test" for sandbox, "production" for live
NODE_ENV=development
